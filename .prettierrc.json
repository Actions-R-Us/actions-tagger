{
   "useTabs": false,
   "semi": true,
   "singleQuote": true,
   "quoteProps": "as-needed",
   "trailingComma": "es5",
   "bracketSpacing": true,
   "bracketSameLine": true,
   "arrowParens": "avoid",
   "htmlWhitespaceSensitivity": "css",
   "proseWrap": "always",
   "overrides": [
      {
         "files": ".importsortrc",
         "options": {
            "parser": "json"
         }
      }
   ]
}
