{
   "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "outDir": "lib",
      "sourceMap": true,
      "alwaysStrict": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "noImplicitAny": true,
      "isolatedModules": true,
      "esModuleInterop": true,
      "noUnusedLocals": true,
      "paths": {
         "@actionstagger/*": ["./src/*"],
         "@actionstagger": ["./src"]
      }
   }
}
