{
   "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "outDir": "lib",
      "alwaysStrict": true,
      "moduleResolution": "node",
      "noImplicitAny": true,
      "esModuleInterop": true,
      "noUnusedLocals": true,
      "charset": "utf-8",
      "baseUrl": ".",
      "paths": {
         "src/*": ["src/*"]
      }
   },
   "typeAcquisition": {
      "enable": true
   }
}
